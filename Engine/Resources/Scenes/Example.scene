meta
{
    version 0.0.1
    name "Example Scene"
    camera Main
}

entity
{
    id Main
    add Transform vec3(0, 0, -5) vec3(0) vec3(0)
    add Camera 0 0.1 100. 90. vec2(0)
}

entity
{
    id Entity
    add Transform vec3(0) vec3(1) vec3(0)
    add Renderer mesh("Objects/cube.obj") shader(unlit)
}